<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Authors>Roman Boykov</Authors>
    <Description>STUN support for Datagrammer</Description>
    <PackageProjectUrl>https://github.com/gendalf90/Datagrammer</PackageProjectUrl>
    <RepositoryUrl>https://github.com/gendalf90/Datagrammer.Stun</RepositoryUrl>
    <PackageTags>udp client datagram datagrammer stun dataflow reactive rx observable</PackageTags>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Version>1.0.2</Version>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Datagrammer" Version="3.1.0" />
    <PackageReference Include="STUN" Version="0.5.0" />
  </ItemGroup>

</Project>
